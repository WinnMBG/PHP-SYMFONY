<!DOCTYPE html>
<html>
    {% for product in products%}
        <h1> {{product.name}} </h1>
        <p>
            <img src={{product.image}} alt=""/>
            <h3>{{product.price}}</h3>
        </p>
        <button onclick={{url('indexA', {slug: {{product.slug}}})}}>Consulter</button>
    {% endfor %}
</html>